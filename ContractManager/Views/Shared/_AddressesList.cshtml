@model IEnumerable<ContractManager.Domain.Address>

<table>
    <tr>
        <th>@Html.DisplayNameFor(model => model.Street)</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Street)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", "Address", new { id=item.Id }, null) |
            @Html.ActionLink("Details", "Details", "Address", new { id=item.Id }, null) |
            @Html.ActionLink("Delete", "Delete", "Address", new { id=item.Id }, null)
        </td>
    </tr>
}

</table>
