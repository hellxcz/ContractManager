@model IEnumerable<ContractManager.Domain.Contact>

<table>
    <tr>
        <th>@Html.DisplayNameFor(model => model.Value)</th>
        @*<th>@Html.DisplayNameFor(model => model.ContactType)</th>*@
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Value)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.ContactType)
        </td>*@
        <td>
            @Html.ActionLink("Edit", "Edit", "Contact", new { id=item.Id }, null) |
            @Html.ActionLink("Details", "Details", "Contact", new { id=item.Id }, null) |
            @Html.ActionLink("Delete", "Delete", "Contact", new { id=item.Id }, null)
        </td>
    </tr>
}

</table>


